{
 "baseUrl": "${localBaseUrl}",
 "key": "confluence-webhook",
 "authentication": {
 "type": "jwt"
 },
 "vendor": {
 "name": "Wikistrat",
 "url": "http://www.wikistrat.com"
 },
 "description": "Atlassian Connect add-on that connects to GeoIP2",
 "name": "GeoIP2",
 "lifecycle": {
 "installed": "/installed",
 "uninstalled": "/uninstalled"
 },
 "scopes": [ "READ", "WRITE" ],
 "modules": {
 "webhooks": [
 { "key": "confluence-webhook",
 "event": "login",
 "url": "geoip/login"
 }
 ]
 }
}
