{
    "baseUrl": "${localBaseUrl}",
    "key": "${addonKey}",
    "authentication": {
        "type": "jwt"
    },
    "vendor": {
        "name": "Wikistrat",
        "url": "http://www.wikistrat.com"
    },
    "description": "Atlassian Connect add-on that connects to GeoIP2",
    "name": "GeoIP2",
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [ "READ" ],
    "modules": {
        "webhooks": [
            {
                "event": "login",
                "url": "/geoip/login",
                "event": "logout",
                "url": "/geoip/logout"
            }
        ],
        "generalPages": [
            {
                "key": "login-report",
                "url": "/login-report",
                "name": {
                    "value": "Login Report"
                }
            }
        ]
    }
}