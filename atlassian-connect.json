{
    "baseUrl": "${localBaseUrl}",
    "key": "${addonKey}",
    "authentication": {
        "type": "jwt"
    },
    "vendor": {
        "name": "Wikistrat",
        "url": "http://www.wikistrat.com"
    },
    "description": "Atlassian Connect add-on that connects to GeoIP2",
    "name": "GeoIP2",
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [ "READ", "WRITE" ],
    "modules": {
        "webhooks": [
            {   "event": "login",  "url": "/geoip/login"   },
            {   "event": "logout", "url": "/geoip/logout"  }
        ],
        "generalPages": [
            {
                "key": "loginreport",
                "url": "/loginreport",
                "name": {
                    "value": "Login Report"
                }
            },
            {
                "key": "test",
                "url": "/assets/testgeneralpage.html",
                "name": {
                    "value": "Test General Page HTML"
                }
            }
        ]
    }
}