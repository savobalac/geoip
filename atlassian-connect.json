{
    "baseUrl": "${localBaseUrl}",
    "key": "${addonKey}",
    "authentication": {
        "type": "jwt"
    },
    "vendor": {
        "name": "Wikistrat",
        "url": "http://www.wikistrat.com"
    },
    "description": "Atlassian Connect add-on that connects to GeoIP2",
    "name": "GeoIP2",
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [ "READ" ],
    "modules": {
        "webhooks": [
            { "event": "login",        "url": "/geoip/login" },
            { "event": "login_failed", "url": "/geoip/ping"  },
            { "event": "logout",       "url": "/geoip/ping"  }
        ]
    }
}
